    <!-- Stylesheet -->
    <link href="{{ asset('build/global/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('build/global/css/jquery-ui.min.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('build/global/css/animate.css')}}" rel="stylesheet" type="text/css">
